<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
   <welcome-file-list> 
       <welcome-file>Login.jsp</welcome-file>  
   </welcome-file-list> 
  
  <!--? Definición del servlet ReverseLogic -->
  <servlet> 
    <servlet-name>ReverseLogic</servlet-name> 
    <servlet-class>ReverseLogic</servlet-class> 
  </servlet> 
  
  <servlet-mapping> 
    <servlet-name>ReverseLogic</servlet-name> 
    <url-pattern>/ReverseLogic</url-pattern>  
  </servlet-mapping> 

  <!--? Definición del servlet Empleado -->
